<?xml version="1.0" encoding="US-ASCII" standalone="yes"?>
<limits>
 <runnerClassName>compbio.runner.clustal.ClustalW</runnerClassName>
	<limit isDefault="true">
		<seqNumber>1000</seqNumber>
		<seqLength>1000</seqLength>
	</limit>
	<limit isDefault="false">
		<preset># LocalEngineExecutionLimit #</preset>
		<seqNumber>30</seqNumber>
		<seqLength>500</seqLength>
	</limit>

	<!-- Limits for presets	-->
	<limit isDefault="false">
		<preset>Disable gap weighting (Speed-oriented)</preset>
		<seqNumber>1000</seqNumber>
		<seqLength>3000</seqLength>
	</limit>

</limits>
